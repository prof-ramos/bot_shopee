# Shopee Affiliate API Credentials
# Copie este arquivo para .env e preencha com suas credenciais

# App ID da Shopee Affiliate API
SHOPEE_APP_ID=seu_app_id_aqui

# App Secret da Shopee Affiliate API
SHOPEE_APP_SECRET=sua_chave_secreta_aqui

# Configurações de API
SHOPEE_ENDPOINT=https://open-api.affiliate.shopee.com.br/graphql

# Configurações opcionais
SHOPEE_API_TIMEOUT=30
SHOPEE_MAX_RETRIES=3
